{
  "plugins": [
    "filenames"
  ],
  "extends": [
    "defaults/configurations/walmart/es6-react"
  ],
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "plugins": ["react"],
  "env": {
    "es6": true
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "jsx-quotes": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-invalid-this": 0,
    "react/jsx-closing-bracket-location": [2, "after-props"],
    "react/jsx-boolean-value": 0,
    "valid-jsdoc": 0
  },
  "globals": {
    "require": true
  }
}
