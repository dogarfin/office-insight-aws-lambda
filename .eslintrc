{
  "plugins": ["filenames"],
  "extends": ["airbnb"],
  "env": {
    "es6": true
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "indent": [2, 2, { "SwitchCase": 1 }],
    "no-invalid-this": 0,
    "valid-jsdoc": 0,
    "object-shorthand": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "implicit-arrow-linebreak": 0,
    "no-prototype-builtins": 0,
    "no-throw-literal": 0,
    "react/destructuring-assignment": 0
  },
  "globals": {
    "require": true
  }
}
