{
  "plugins": ["filenames"],
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "es6": true
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-invalid-this": 0,
    "valid-jsdoc": 0
  },
  "globals": {
    "require": true
  }
}
