{
  "name": "oi-lambda",
  "version": "0.0.1",
  "description": "Office Insight Lambda",
  "repository": {
    "type": "git",
    "url": "git@github.com:dogarfin/office-insight-aws-lambda.git"
  },
  "main": "application.js",
  "scripts": {
    "start": "webpack --config webpack.config.js --progress --colors --watch",
    "clean": "rimraf public/dist",
    "build": "webpack",
    "lint": "eslint --ext .js,.jsx src",
    "yolo": "rm mock-data.json",
    "test": "npm run lint",
    "zip": "zip -r dist/lambda.zip dist/bundle.js",
    "prod": "npm run build && npm run zip",
    "try": "node bin/try-handler"
  },
  "author": "Sean Owiecki",
  "contributors": [
    {
      "name": "Sean Owiecki",
      "email": "sean.owiecki@slalom.com"
    }
  ],
  "license": "none",
  "dependencies": {
    "foreman": "^1.4.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-config-defaults": "^7.0.1",
    "eslint-formatter-pretty": "^0.2.2",
    "eslint-plugin-filenames": "^1.0.0",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "nodemon": "^1.8.0",
    "replace": "^0.3.0",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.2"
  }
}
